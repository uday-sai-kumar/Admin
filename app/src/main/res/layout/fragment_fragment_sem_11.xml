        <TextView
            android:fontFamily="@font/open_sans_light"
            android:text="@string/externals"
            android:textSize="15sp" />

        <TextView
            android:fontFamily="@font/open_sans_light"
            android:text="@string/totalmarks"
            android:textSize="15sp" />
    </TableRow>
    <TableRow>

        <TextView
            android:id="@+id/eng"
            android:fontFamily="@font/open_sans_light"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/engi"
            android:fontFamily="@font/open_sans_light"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/engex"
            android:fontFamily="@font/open_sans_light"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/engtotal"
            android:fontFamily="@font/open_sans_light"
            android:textSize="16sp"
            android:textStyle="bold" />
    </TableRow>
    <TableRow>

        <TextView
