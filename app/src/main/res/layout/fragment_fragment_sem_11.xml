    android:layout_height="wrap_content"
    android:stretchColumns="*"
    >
    <TableRow>

        <TextView
            android:fontFamily="@font/open_sans_light"
            android:text="@string/subject"
            android:textSize="15sp" />

        <TextView
            android:fontFamily="@font/open_sans_light"
